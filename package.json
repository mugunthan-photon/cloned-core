{
  "name": "yoda-core-components",
  "version": "6.0.7",
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "scripts": {
    "publish:package": "babel-node ./bin/run publish",
    "prepublish:package": "npm run svg-setup",
    "_storybook": "start-storybook -p 9001 -s src/assets",
    "pre_storybook": "npm run svg-setup",
    "lint": "babel-node ./bin/run lint",
    "clean": "babel-node ./bin/run clean",
    "build": "babel-node ./bin/run build",
    "_test": "cross-env NODE_PATH=./src _mocha",
    "_watch:test": "NODE_PATH=./src _mocha --watch src/*.jsx",
    "_coverage": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha",
    "dev": "parallelshell \"npm run _watch:test\" \"npm run _storybook\"",
    "svg-generate-sprite": "svg-sprite-generate -d './svgGen' -o './src/assets/sprite.svg'",
    "svg-optimize": "svgo -f './src/assets/svg' -o './svgGen'",
    "svg-setup": "npm run _svg-optimize && npm run _svg-generate-sprite",
    "create": "node ./CreateComponent.js ./src/components",
    "_test:watch": "cross-env NODE_PATH=./src _mocha --watch src/*.jsx",
    "_coverage:check": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/babel-istanbul check-coverage",
    "_coverage:report": "open coverage/lcov-report/index.html",
    "sanitycheck": "npm run svg-setup && npm run lint && npm run _coverage && npm run _coverage:check",
    "precommit": "npm run sanitycheck",
    "_svg-generate-sprite": "svg-sprite-generate -d './svgGen' -o './src/assets/sprite.svg'",
    "_svg-optimize": "svgo -f './src/assets/svg' -o './svgGen'",
    "_coverage-sonar": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/babel-istanbul cover --config .istanbul-sonar.yml _mocha",
    "_coverage:check-sonar": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/babel-istanbul check-coverage --config .istanbul-sonar.yml",
    "start": "node ./build/server/scripts/server.js | ./node_modules/.bin/bunyan",
    "sanitychecksonar": "npm run svg-setup && npm run lint && npm run _coverage-sonar && npm run _coverage:check-sonar",
    "watch": "npm run svg-setup; npm run clean; webpack --watch --config webpack/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.jcpenney.com:7999/jcp-rwd/yoda-common.git"
